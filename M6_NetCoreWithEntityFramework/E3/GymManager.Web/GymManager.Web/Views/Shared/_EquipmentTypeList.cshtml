@model List<GymManager.Core.EquipmentTypes.EquipmentType>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col"></th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var equipmentType in Model)
        {
            <tr>
                <th scope="row">@equipmentType.EquipmentName</th>
                <td>
                    <a asp-action="Edit" class="btn btn-outline-info" asp-route-equipmentId="@equipmentType.Id">Edit</a>
                </td>
                <td>
                    <a asp-action="Delete" class="btn btn-outline-danger" asp-route-equipmentId="@equipmentType.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
