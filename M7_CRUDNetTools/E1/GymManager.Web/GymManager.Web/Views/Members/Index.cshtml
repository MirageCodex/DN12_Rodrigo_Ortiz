@model GymManager.Web.Models.MemberListViewModel


@{
    DateTime now = DateTime.Now;
    Layout = "/Views/Shared/_Layout.cshtml";
}


}
<div class="container">
    <h1>Welcome to Gym Manager</h1>
    <div class="row">
        <div class="col-6">
        <p>New Members of the Week : @Model.NewMembersCount</p>
        </div>
        <div class="col-6">
            <a asp-action="Create" class="btn btn-outline-success float-end">Add Member</a>
        </div>
    </div>
    
    @await Html.PartialAsync("/Views/Shared/_MemberList.cshtml", Model.Members)
</div>

